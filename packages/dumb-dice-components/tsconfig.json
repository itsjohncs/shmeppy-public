{
    "compilerOptions": {
        "target": "ES6",
        "lib": ["ES2017", "DOM"],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "composite": true,
        "declaration": true,
        "outDir": "dist",
        "jsx": "react-jsx"
    }
}
